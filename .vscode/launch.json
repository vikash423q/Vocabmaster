{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: AI Agent Service",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "ai_agent_service.app.main:app",
                "--host",
                "0.0.0.0",
                "--port",
                "8001",
                "--reload"
            ],
            "python": "${workspaceFolder}/backend/venv/bin/python",
            "cwd": "${workspaceFolder}/ai_agent_service",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "jinja": true
        },
        {
            "name": "Python Debugger: Backend API",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "app.main:app",
                "--host",
                "0.0.0.0",
                "--port",
                "8000",
                "--reload"
            ],
            "python": "${workspaceFolder}/backend/venv/bin/python",
            "cwd": "${workspaceFolder}/backend",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/backend"
            },
            "jinja": true
        }
    ]
}